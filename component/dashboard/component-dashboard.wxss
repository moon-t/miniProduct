/* component/dashboard/component-dashboard.wxss */
@import "../../style/iconfont.wxss";
.dashboard-component{
	height: 100%;
	width: 100%;    
	position: relative;
  z-index: 1;
	background-color: #241f54;
}
.dashboard-top{
	display: flex;
	height: 141px;    
	background-repeat: no-repeat;
	background-size: cover;
	width: 100%;
	background-image: url(data:image/jpeg;base64,);
	align-items:flex-start;
	-webkit-box-pack: justify;
}
.dash-item{      
	box-sizing: border-box;
	padding: 40px 0 0 0;
	height: 100%;
	width: calc((100%) / 3);
	float: left;
	text-align: center;
	margin-bottom: 10px;
}
.dash-num{
	font-size: 34px;
	font-weight: 600;
	color: #FFF;
}
.dash-text{
	margin: 1em 0;
	font-size: 14px;
	color: #b1a7c1;
}
.dashboard-content{
	margin: 35px 0;
	padding: 0 5px;
	height: calc(100% - 141px);
}
.content-item{
	float: left;
	border-radius: 10px;
	background: #1d0a3b;
	text-align: center;
	margin: 8px;
	padding: 10rpx;
	/* height: calc((100% - 15px*2) / 3); */
	width: calc((100% - 78px) / 3);
}
.iconfont{
	margin: 15rpx 0;
	color: #3dcfc1;
	font-size: 100rpx;
}
.content-text{
	font-size: 16px;
	color: #b1a7c1;
	margin-bottom: 12px;
}