/**app.wxss**/
@import "/style/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
	width: 100%;
	height: 100%;
}
.container-page{
	padding: 10px;
}
.container-page-label{
	float: left;
	width: 100%;
	height: 28px;
	line-height: 28px;
	font-size: 28rpx;
}
.container-page-btn{
		width: 150rpx;
		height: 70rpx;
		float: left;
		font-size: 24rpx;
		padding: 0;
		border: 1px solid #dcdcdc;
		background: #fff;
		border-radius: 4px;
		margin: 15rpx 10rpx 15rpx 0;
		/* margin-left: 0; */
		text-align: center;
}
.container-page-btn-selected{
		/* border: 1px solid #06a380; */
		border: 1px solid #3dcfc1;
}
.container-page-input{
	float: left;
	border: 1px solid #dcdcdc;
	border-radius: 4px;
	font-size: 28rpx;
	margin: 15rpx 10rpx 15rpx 0;
	padding-left: 5px;
}